{
    "name": "limonlabs/bigcommerce",
    "type": "package",
    "description": "Bigcommerce package for Laravel",
    "version": "0.0.2",
    "autoload": {
        "psr-4": {
            "Limonlabs\\Bigcommerce\\": "src/"
        }
    },
    "authors": [
        {
            "name": "LimonLabs",
            "email": "dev@limonlabs.dev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/cashier": "^15.6",
        "stancl/tenancy": "3.8.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Limonlabs\\Bigcommerce\\Providers\\LimonlabsBigcommerceProvider",
                "Stancl\\Tenancy\\TenancyServiceProvider"
            ]
        }
    }
}
